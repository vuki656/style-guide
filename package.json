{
    "name": "@dvukovic/style-guide",
    "version": "0.3.45",
    "description": "My own style guide",
    "repository": {
        "type": "git",
        "url": "https://github.com/vuki656/style-guide"
    },
    "license": "MIT",
    "author": {
        "name": "Domagoj Vukovic"
    },
    "type": "commonjs",
    "files": [
        "src/eslint",
        "src/cspell",
        "src/graphql",
        "src/prettier",
        "src/stylelint",
        "src/package-json"
    ],
    "scripts": {
        "lint": "yarn lint:prettier && yarn lint:eslint && yarn lint:stylelint && yarn lint:spell && yarn lint:package-json",
        "lint:eslint": "eslint",
        "lint:fix": "yarn lint:eslint --fix && yarn lint:prettier --write  && yarn lint:stylelint --fix && yarn lint:spell && yarn lint:package-json",
        "lint:package-json": "npmPkgJsonLint --configFile ./.packagerc.js .",
        "lint:prettier": "prettier --log-level=warn --check --cache .",
        "lint:spell": "cspell --config ./.cspellrc.js --no-progress --no-summary --unique '**'",
        "lint:stylelint": "stylelint ./**/*.css --cache",
        "release": "release-it"
    },
    "dependencies": {
        "@eslint-community/eslint-plugin-eslint-comments": "4.4.1",
        "@eslint/compat": "^1.2.4",
        "@graphql-eslint/eslint-plugin": "3.20.1",
        "@next/eslint-plugin-next": "15.0.4",
        "@prettier/plugin-xml": "3.4.1",
        "@rimac-technology/eslint-plugin": "1.5.0",
        "@stylistic/eslint-plugin": "2.11.0",
        "@typescript-eslint/parser": "8.17.0",
        "eslint-plugin-etc": "2.0.3",
        "eslint-plugin-import-x": "4.5.0",
        "eslint-plugin-jest": "28.9.0",
        "eslint-plugin-mobx": "0.0.13",
        "eslint-plugin-n": "17.14.0",
        "eslint-plugin-promise": "7.2.1",
        "eslint-plugin-react": "7.37.2",
        "eslint-plugin-react-hooks": "5.1.0",
        "eslint-plugin-security-node": "1.1.4",
        "eslint-plugin-simple-import-sort": "12.1.1",
        "eslint-plugin-sonarjs": "1.0.4",
        "eslint-plugin-sort-destructure-keys": "2.0.0",
        "eslint-plugin-sort-keys-fix": "1.1.2",
        "eslint-plugin-typescript-sort-keys": "3.3.0",
        "eslint-plugin-unicorn": "56.0.1",
        "eslint-plugin-unused-imports": "4.1.4",
        "prettier-plugin-embed": "0.4.15",
        "prettier-plugin-jsdoc": "1.3.0",
        "prettier-plugin-packagejson": "2.5.6",
        "prettier-plugin-prisma": "5.0.0",
        "prettier-plugin-sh": "0.14.0",
        "prettier-plugin-sql": "0.18.1",
        "prettier-plugin-toml": "2.0.1",
        "stylelint-order": "6.0.4",
        "typescript-eslint": "8.17.0"
    },
    "devDependencies": {
        "@types/jest": "29.5.14",
        "@types/node": "22.10.1",
        "@types/react": "19.0.1",
        "cspell": "8.16.1",
        "eslint": "9.16.0",
        "graphql": "16.9.0",
        "jest": "29.7.0",
        "npm-package-json-lint": "8.0.0",
        "prettier": "3.4.2",
        "react": "19.0.0",
        "release-it": "17.10.0",
        "stylelint": "16.11.0",
        "typescript": "5.7.2"
    },
    "peerDependencies": {
        "cspell": "8",
        "eslint": "8.57.0",
        "npm-package-json-lint": "8",
        "prettier": "3",
        "stylelint": "16"
    },
    "peerDependenciesMeta": {
        "cspell": {
            "optional": true
        },
        "eslint": {
            "optional": true
        },
        "npm-package-json-lint": {
            "optional": true
        },
        "prettier": {
            "optional": true
        },
        "stylelint": {
            "optional": true
        }
    },
    "packageManager": "yarn@4.5.3",
    "engines": {
        "node": ">=20.0.0"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org"
    }
}
