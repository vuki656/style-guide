{
    "name": "@dvukovic/style-guide",
    "version": "0.3.93",
    "description": "My own style guide",
    "repository": {
        "type": "git",
        "url": "https://github.com/vuki656/style-guide"
    },
    "license": "MIT",
    "author": {
        "name": "Domagoj Vukovic"
    },
    "type": "module",
    "files": [
        "src/eslint",
        "src/cspell",
        "src/prettier",
        "src/stylelint",
        "src/package-json"
    ],
    "scripts": {
        "lint": "yarn lint:eslint && yarn lint:prettier && yarn lint:stylelint && yarn lint:spell && yarn lint:package-json",
        "lint:eslint": "eslint . --cache --concurrency=auto",
        "lint:fix": "yarn lint:eslint --fix --concurrency=auto --cache && yarn lint:prettier --write  && yarn lint:stylelint --fix && yarn lint:spell && yarn lint:package-json",
        "lint:package-json": "npmPkgJsonLint --configFile ./.packagerc.js .",
        "lint:prettier": "prettier --log-level=warn --check --cache .",
        "lint:spell": "cspell --config ./.cspellrc.js --no-progress --no-summary --unique '**'",
        "lint:stylelint": "stylelint ./**/*.css --cache",
        "release": "release-it",
        "test": "vitest run"
    },
    "dependencies": {
        "@eslint-community/eslint-plugin-eslint-comments": "4.5.0",
        "@next/eslint-plugin-next": "16.0.6",
        "@prettier/plugin-xml": "3.4.2",
        "@rimac-technology/eslint-plugin": "1.5.0",
        "@stylistic/eslint-plugin": "5.6.1",
        "@typescript-eslint/parser": "8.48.0",
        "@vitest/eslint-plugin": "1.5.1",
        "eslint-plugin-es-x": "9.2.0",
        "eslint-plugin-etc": "2.0.3",
        "eslint-plugin-import-x": "4.16.1",
        "eslint-plugin-jest": "29.2.1",
        "eslint-plugin-mobx": "0.0.13",
        "eslint-plugin-n": "17.23.1",
        "eslint-plugin-playwright": "2.4.0",
        "eslint-plugin-promise": "7.2.1",
        "eslint-plugin-react": "7.37.5",
        "eslint-plugin-react-hooks": "7.0.1",
        "eslint-plugin-security-node": "1.1.4",
        "eslint-plugin-simple-import-sort": "12.1.1",
        "eslint-plugin-sonarjs": "3.0.5",
        "eslint-plugin-sort-destructure-keys": "2.0.0",
        "eslint-plugin-sort-keys-fix": "1.1.2",
        "eslint-plugin-storybook": "10.1.2",
        "eslint-plugin-typescript-sort-keys": "3.3.0",
        "eslint-plugin-unicorn": "62.0.0",
        "eslint-plugin-unused-imports": "4.3.0",
        "globals": "16.5.0",
        "prettier-plugin-embed": "0.5.0",
        "prettier-plugin-jsdoc": "1.7.0",
        "prettier-plugin-packagejson": "2.5.20",
        "prettier-plugin-prisma": "5.0.0",
        "prettier-plugin-sh": "0.18.0",
        "prettier-plugin-sql": "0.19.2",
        "prettier-plugin-tailwindcss": "0.7.2",
        "prettier-plugin-toml": "2.0.6",
        "stylelint-no-unused-selectors": "1.0.40",
        "stylelint-order": "7.0.0",
        "typescript-eslint": "8.48.0"
    },
    "devDependencies": {
        "@storybook/react": "10.1.2",
        "@types/eslint": "9.6.1",
        "@types/jest": "30.0.0",
        "@types/node": "24.10.1",
        "@types/react": "19.2.7",
        "cspell": "9.3.2",
        "eslint": "9.39.1",
        "jest": "30.2.0",
        "npm-package-json-lint": "9.0.0",
        "prettier": "3.7.3",
        "react": "19.2.0",
        "release-it": "19.0.6",
        "storybook": "^10.1.2",
        "stylelint": "16.26.1",
        "typescript": "5.9.3",
        "vitest": "^4.0.14"
    },
    "peerDependencies": {
        "cspell": "9",
        "eslint": "^9",
        "npm-package-json-lint": "9",
        "prettier": "3",
        "stylelint": "16"
    },
    "peerDependenciesMeta": {
        "cspell": {
            "optional": true
        },
        "eslint": {
            "optional": true
        },
        "npm-package-json-lint": {
            "optional": true
        },
        "prettier": {
            "optional": true
        },
        "stylelint": {
            "optional": true
        }
    },
    "packageManager": "yarn@4.12.0",
    "engines": {
        "node": ">=20.0.0"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org"
    }
}
