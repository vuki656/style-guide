{
    "name": "@dvukovic/style-guide",
    "version": "0.10.3",
    "description": "My own style guide",
    "repository": {
        "type": "git",
        "url": "https://github.com/vuki656/style-guide"
    },
    "license": "MIT",
    "author": {
        "name": "Domagoj Vukovic"
    },
    "type": "module",
    "exports": {
        "./cspell": {
            "types": "./dist/cspell/index.d.ts",
            "default": "./src/cspell/index.js"
        },
        "./cspell/base.txt": "./src/cspell/base.txt",
        "./eslint": {
            "types": "./dist/eslint/index.d.ts",
            "default": "./src/eslint/index.js"
        },
        "./knip": {
            "types": "./src/knip/index.d.ts",
            "default": "./src/knip/index.js"
        },
        "./prettier": {
            "types": "./dist/prettier/index.d.ts",
            "default": "./src/prettier/index.js"
        },
        "./stylelint": {
            "types": "./dist/stylelint/index.d.ts",
            "default": "./src/stylelint/index.js"
        }
    },
    "bin": "./src/cli/index.js",
    "files": [
        "dist",
        "src/cli",
        "src/cspell",
        "src/eslint",
        "src/knip",
        "src/package-json",
        "src/prettier",
        "src/stylelint"
    ],
    "scripts": {
        "build": "tsc",
        "lint": "yarn lint:eslint && yarn lint:prettier && yarn lint:stylelint && yarn lint:cspell && yarn lint:knip",
        "lint:cspell": "cspell --no-progress --no-summary --unique '**'",
        "lint:eslint": "eslint . --cache --concurrency=auto",
        "lint:fix": "yarn lint:eslint --fix && yarn lint:prettier --write && yarn lint:stylelint --fix && yarn lint:cspell && yarn lint:knip",
        "lint:knip": "knip",
        "lint:prettier": "prettier --check --cache .",
        "lint:stylelint": "stylelint ./**/*.css --cache",
        "release": "release-it",
        "test": "vitest run"
    },
    "dependencies": {
        "@clack/prompts": "0.10.0",
        "@eslint-community/eslint-plugin-eslint-comments": "4.5.0",
        "@eslint/config-helpers": "0.4.2",
        "@next/eslint-plugin-next": "16.1.0",
        "@rimac-technology/eslint-plugin": "1.5.0",
        "@stylistic/eslint-plugin": "5.6.1",
        "@typescript-eslint/parser": "8.50.0",
        "@vitest/eslint-plugin": "1.5.4",
        "eslint-plugin-baseline-js": "0.4.2",
        "eslint-plugin-import-x": "4.16.1",
        "eslint-plugin-jest": "29.4.0",
        "eslint-plugin-mobx": "0.0.13",
        "eslint-plugin-n": "17.23.1",
        "eslint-plugin-package-json": "0.88.1",
        "eslint-plugin-playwright": "2.4.0",
        "eslint-plugin-promise": "7.2.1",
        "eslint-plugin-react": "7.37.5",
        "eslint-plugin-react-hooks": "7.0.1",
        "eslint-plugin-security-node": "1.1.4",
        "eslint-plugin-simple-import-sort": "12.1.1",
        "eslint-plugin-sonarjs": "3.0.5",
        "eslint-plugin-sort-destructure-keys": "2.0.0",
        "eslint-plugin-sort-keys-fix": "1.1.2",
        "eslint-plugin-storybook": "10.1.10",
        "eslint-plugin-typescript-sort-keys": "3.3.0",
        "eslint-plugin-unicorn": "62.0.0",
        "eslint-plugin-unused-imports": "4.3.0",
        "globals": "16.5.0",
        "jsonc-eslint-parser": "2.4.2",
        "prettier-plugin-embed": "0.5.1",
        "prettier-plugin-jsdoc": "1.8.0",
        "prettier-plugin-packagejson": "2.5.20",
        "prettier-plugin-prisma": "5.0.0",
        "prettier-plugin-sh": "0.18.0",
        "prettier-plugin-sql": "0.19.2",
        "prettier-plugin-tailwindcss": "0.7.2",
        "prettier-plugin-toml": "2.0.6",
        "stylelint-no-unused-selectors": "1.0.40",
        "stylelint-order": "7.0.0",
        "typescript-eslint": "8.50.0"
    },
    "devDependencies": {
        "@release-it/conventional-changelog": "10.0.4",
        "@types/node": "24.10.4",
        "cspell": "9.4.0",
        "eslint": "9.39.2",
        "knip": "5.82.1",
        "prettier": "3.7.4",
        "release-it": "19.1.0",
        "stylelint": "16.26.1",
        "typescript": "5.9.3",
        "vitest": "4.0.16"
    },
    "peerDependencies": {
        "cspell": "9",
        "eslint": "^9",
        "knip": "5",
        "prettier": "3",
        "stylelint": "16"
    },
    "peerDependenciesMeta": {
        "cspell": {
            "optional": true
        },
        "eslint": {
            "optional": true
        },
        "knip": {
            "optional": true
        },
        "prettier": {
            "optional": true
        },
        "stylelint": {
            "optional": true
        }
    },
    "packageManager": "yarn@4.12.0",
    "engines": {
        "node": ">=24.0.0"
    },
    "volta": {
        "node": "24.4.1"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org"
    }
}
